echo "-----&gt; haproxy-1.6.9"
#!/usr/bin/env bash∆í
echo "-----&gt; haproxy-1.6.9"

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3
cd $BUILD_DIR
curl -O http://nginx.org/download/nginx-1.10.1.tar.gz
tar --strip-components=1 -zxvf nginx-1.10.1.tar.gz
mkdir $CACHE_DIR/nginx
./configure  --prefix=$CACHE_DIR/nginx --error-log-path=$CACHE_DIR/nginx/errors.log --pid-path=$CACHE_DIR/nginx/nginx.pid --sbin-path=$CACHE_DIR/nginx/sbin --conf-path=$CACHE_DIR/nginx/nginx.conf
make install 