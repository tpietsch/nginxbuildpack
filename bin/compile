echo "-----&gt; haproxy-1.6.9"
#!/usr/bin/env bash∆í
echo "-----&gt; haproxy-1.6.9"

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3
cd $BUILD_DIR

curl -O https://openresty.org/download/openresty-1.11.2.1.tar.gz
tar --strip-components=1 -zxvf openresty-1.11.2.1.tar.gz
rm  openresty-1.11.2.1.tar.gz
mkdir nginx
./configure  --prefix=/app/nginx --error-log-path=/app/nginx/errors.log --pid-path=/app/nginx/nginx.pid --sbin-path=$BUILD_DIR/nginx/sbin --conf-path=/app/nginx.conf
make install 